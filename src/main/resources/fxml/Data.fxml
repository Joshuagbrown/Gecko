<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team6.controller.DataController">
   <center>
      <TableView fx:id="table" minHeight="0.0" minWidth="0.0" onMouseClicked="#clickItem">
        <placeholder>
          <Label text="No charging station data yet, import some charging station data and you'll see it here!" />
        </placeholder>
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="71.0" text="Name" />
          <TableColumn fx:id="address" prefWidth="75.0" text="Address" />
          <TableColumn fx:id="chargers" prefWidth="75.0" text="Chargers" />
          <TableColumn fx:id="noOfCarPark" prefWidth="75.0" text="No. of Car Park" />
          <TableColumn prefWidth="75.0" text="Latitude" fx:id="xcolumn" />
          <TableColumn fx:id="ycolumn" prefWidth="75.0" text="Longitude" />
        </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
